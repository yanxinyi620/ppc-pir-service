<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE Configuration><Configuration monitorInterval="5">    <Properties>        <!-- log level -->        <Property name="LOG_LEVEL">info</Property>         log location        		<Property name="LOG_HOME">./logs</Property><!--        <Property name="LOG_HOME">/data/app/logs/wedpr-pir</Property>-->        <!-- log name -->        <Property name="APP_NAME">wedpr-pir</Property>        <property name="MONITOR_LOG_PATTERN" value="[%-p][%d{yyyy-MM-dd HH:mm:ss.SSS}][%t][%F:%L][%m]%n"/>    </Properties>    <Appenders>        <RollingFile name="appLog" fileName="${LOG_HOME}/${APP_NAME}.log"                     filePattern="${LOG_HOME}/${APP_NAME}.log.%d{yyyy-MM-dd}.%i.log.gz">            <PatternLayout pattern="[%-p][%-t][%d{yyyy-MM-dd HH:mm:ss sss}][${sys:PID}][%F:%L]%m%n"/>            <Filters>                <ThresholdFilter level="warning" onMatch="DENY" onMismatch="NEUTRAL"/>                <ThresholdFilter level="info" onMatch="ACCEPT" onMismatch="DENY"/>            </Filters>            <Policies>                <TimeBasedTriggeringPolicy />                <!-- 日志文件大小 -->                <SizeBasedTriggeringPolicy size="1000 MB" />            </Policies>            <DefaultRolloverStrategy max="100" />        </RollingFile>        <RollingFile name="errorLog" fileName="${LOG_HOME}/${APP_NAME}.log"                     filePattern="${LOG_HOME}/${APP_NAME}.log.%d{yyyy-MM-dd}.%i.log.gz">            <PatternLayout pattern="[%-p][%d{yyyy-MM-dd HH:mm:ss sss}][${sys:PID}][%F:%L][OnError]%m%n"/>            <ThresholdFilter level="error" onMatch="ACCEPT" onMismatch="DENY"/>            <Policies>                <TimeBasedTriggeringPolicy />                <!-- 日志文件大小 -->                <SizeBasedTriggeringPolicy size="1000 MB" />            </Policies>            <DefaultRolloverStrategy max="100" />        </RollingFile>        <!--这个OnWarning前缀没有生效，需要日志自行打印-->        <RollingFile name="warningLog" fileName="${LOG_HOME}/${APP_NAME}.log"                     filePattern="${LOG_HOME}/${APP_NAME}.log.%d{yyyy-MM-dd}.%i.log.gz">            <PatternLayout pattern="[%-p][%d{yyyy-MM-dd HH:mm:ss sss}][${sys:PID}][%F:%L][OnWarning]%m%n"/>            <Filters>                <ThresholdFilter level="error" onMatch="DENY" onMismatch="NEUTRAL"/>                <ThresholdFilter level="warning" onMatch="ACCEPT" onMismatch="DENY"/>            </Filters>            <Policies>                <TimeBasedTriggeringPolicy />                <!-- 日志文件大小 -->                <SizeBasedTriggeringPolicy size="1000 MB" />            </Policies>            <DefaultRolloverStrategy max="100" />        </RollingFile>        <RollingFile name="monitorLog" fileName="${LOG_HOME}/appmonitor.log"                     filePattern="${LOG_HOME}/appmonitor.%d{yyyy-MM-dd}.log.gz">            <PatternLayout pattern="${MONITOR_LOG_PATTERN}"/>            <Policies>                <TimeBasedTriggeringPolicy />                <SizeBasedTriggeringPolicy size="200 MB" />            </Policies>            <DefaultRolloverStrategy max="24" />        </RollingFile>        <Console name="console" target="SYSTEM_OUT">            <PatternLayout pattern="[%-p][%d{yyyy-MM-dd HH:mm:ss sss}][${sys:PID}][%F:%L][%X{SRC_BIZ_SEQ}][%X{S}]%m%n"/>            <ThresholdFilter level="fatal" onMatch="ACCEPT" onMismatch="DENY"/>        </Console>    </Appenders>    <Loggers>        <Logger name="org.springframework" level="${LOG_LEVEL}" additivity="false">            <appender-ref ref="appLog"/>            <appender-ref ref="console"/>        </Logger>        <Logger name="org.mybatis" level="${LOG_LEVEL}" additivity="false">            <appender-ref ref="appLog"/>            <appender-ref ref="console"/>        </Logger>        <Logger name="org.apache.ibatis" level="${LOG_LEVEL}" additivity="false">            <appender-ref ref="appLog"/>            <appender-ref ref="console"/>        </Logger>        <Logger name="com.webank.wedpr" level="${LOG_LEVEL}" additivity="false">            <appender-ref ref="appLog"/>            <appender-ref ref="errorLog"/>            <appender-ref ref="warningLog"/>            <appender-ref ref="console"/>        </Logger>        <Logger name="org.fisco.bcos" level="${LOG_LEVEL}" additivity="false">            <appender-ref ref="appLog"/>            <appender-ref ref="warningLog"/>            <appender-ref ref="errorLog"/>            <appender-ref ref="console"/>        </Logger>        <Logger name="monitor" level="trace" additivity="false">            <appender-ref ref="monitorLog"/>        </Logger>        <root level="${LOG_LEVEL}" additivity="false">            <appender-ref ref="appLog"/>            <appender-ref ref="warningLog"/>            <appender-ref ref="errorLog"/>            <appender-ref ref="console"/>        </root>    </Loggers></Configuration>